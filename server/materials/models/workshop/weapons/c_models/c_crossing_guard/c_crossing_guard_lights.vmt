"VertexLitGeneric"
{
	"$baseTexture" "models/workshop/weapons/c_models/c_crossing_guard/c_crossing_guard_lights"
	
	"$phong" "1"
	"$phongexponent" "10"
	"$phongboost" ".2"	
	"$lightwarptexture" "models/lightwarps/weapon_lightwarp"
	"$phongfresnelranges"	"[.3 .5 3]"
	"$halflambert" "0"
	
	"$rimlight" "1"					
	"$rimlightexponent" "2"		
	"$rimlightboost" "1"
	"$glowcolor" "1"

	"$selfillum" "1"
	"$cloakPassEnabled" "1"

	"$yellow" "0"

	"Proxies"
	{
		"invis"
		{
		}
		"AnimatedTexture"
		{
			"animatedtexturevar" "$basetexture"
			"animatedtextureframenumvar" "$frame"
			"animatedtextureframerate" 1
		}

		"ModelGlowColor"
		{
			"resultVar" "$glowcolor"
		}
		"Equals"
		{
			"srcVar1"  "$glowcolor"
			"resultVar" "$color2"
		}
		"YellowLevel"
		{
			"resultVar" "$yellow"
		}
		"Multiply"
		{
			"srcVar1" "$color2"
			"srcVar2" "$yellow"
			"resultVar" "$color2"
		}
	}
}