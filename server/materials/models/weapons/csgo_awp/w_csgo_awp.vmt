"VertexLitGeneric"
{
	"$basetexture"	"models/weapons/csgo_awp/w_csgo_awp"
	"$bumpmap" "models/weapons/csgo_awp/w_csgo_awp_normal"

	"$phong" "1"
	"$phongexponent" "20"
	"$phongboost" "5"	
	"$lightwarptexture" "models\lightwarps\weapon_lightwarp"
	"$phongfresnelranges"	"[.125 .25 .25]"
	
	"$normalalphaphongmask" "1"

	"$tempvar" "0"

	"$basephongexponent" "20"
	"$basephongboost" "6"

	"$commadd_phongexponent" "30"
	"$commadd_phongboost" 	"2.5"
	"$commweapon"		"0"

	"$rimlight" "1"
	"$rimlightexponent" "4"	
	"$rimlightboost" "1"



	"$glowcolor" "1"

	// Cloaking
	"$cloakPassEnabled" "1"
	"$sheenPassEnabled" "1"

	"$sheenmap" 		"cubemaps\cubemap_sheen001"
	"$sheenmapmask" 		"Effects\AnimatedSheen\animatedsheen0"
	"$sheenmaptint" 		"[ 1 1 1 ]"
	"$sheenmapmaskframe" 	"0"
	"$sheenindex" 		"0"

	"$yellow" "0"

	"Proxies"
	{
		"AnimatedWeaponSheen"
		{
			"animatedtexturevar" 		"$sheenmapmask"
			"animatedtextureframenumvar" 	"$sheenmapmaskframe"
			"animatedtextureframerate" 		"40"
		}
		"invis"
		{
		}
		"ModelGlowColor"
		{
			"resultVar" "$glowcolor"
		}
		"Equals"
		{
			"srcVar1"  "$glowcolor"
			"resultVar" "$selfillumtint"
		}
		"Equals"
		{
			"srcVar1"  "$glowcolor"
			"resultVar" "$color2"
		}
		"YellowLevel"
		{
			"resultVar" "$yellow"
		}
		"Multiply"
		{
			"srcVar1" "$color2"
			"srcVar2" "$yellow"
			"resultVar" "$color2"
		}
	}
}