"VertexLitGeneric"
{
	$basetexture 			"models\weapons\stattrack\stattrack_dial"
	$basealphaenvmapmask 	"1"
	
	"$phong" "1"
	"$phongboost" "1"
	"$phongexponent" "250"
	"$lightwarptexture" "models\buildables\sentry1\Sentry1_lightwarp"
	"$phongfresnelranges"	"[.5 1.5 10]"	
	
	$envmap 				"env_cubemap"
	$envmaptint				"[0.1 0.1 0.1]"

	// Cloaking
	"$cloakPassEnabled" "1"

	"$yellow"		"0"
	"$glowcolor" "1"

	Proxies
	{
		StatTrakDigit
		{
			resultVar	$frame
			trimzeros		0
			displayDigit		0
		}
		StatTrakIllum
		{
			resultVar	$color
			minVal		0.5
			maxVal		1.5
		}
		"invis"
		{
		}
		"AnimatedTexture"
		{
			"animatedtexturevar" "$detail"
			"animatedtextureframenumvar" "$detailframe"
			"animatedtextureframerate" 30
		}
		"BurnLevel"
		{
			"resultVar" "$detailblendfactor"
		}
		"ModelGlowColor"
		{
			"resultVar" "$glowcolor"
		}
		"Equals"
		{
			"srcVar1"  "$glowcolor"
			"resultVar" "$selfillumtint"
		}
		"Equals"
		{
			"srcVar1"  "$glowcolor"
			"resultVar" "$color2"
		}
		"YellowLevel"
		{
			"resultVar" "$yellow"
		}
		"Multiply"
		{
			"srcVar1" "$color2"
			"srcVar2" "$yellow"
			"resultVar" "$color2"
		}
	}
}